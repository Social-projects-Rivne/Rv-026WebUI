{
  "root": true,
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "no-console": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 1,
    "react/prefer-stateless-function": 0
  }
}